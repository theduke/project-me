<h1>Listing time_reports</h1>

<table>
  <tr>
    <th>Started</th>
    <th>Stopped</th>
    <th>Task</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @time_reports.each do |time_report| %>
  <tr>
    <td><%= time_report.started %></td>
    <td><%= time_report.stopped %></td>
    <td><%= time_report.task %></td>
    <td><%= link_to 'Show', time_report %></td>
    <td><%= link_to 'Edit', edit_time_report_path(time_report) %></td>
    <td><%= link_to 'Destroy', time_report, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Time report', new_time_report_path %>
